{
  "name": "web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "TZ=UTC vitest",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "dev:test": ". ./.env && VITE_TEMPORAL_UI_BUILD_TARGET=local VITE_MODE=test vite dev --port 3000",
    "codegen": "export NODE_OPTIONS=\"--loader ts-node/esm\" && node codegen.ts && graphql-codegen-esm --config codegen.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.2",
    "@graphql-codegen/client-preset": "^1.2.4",
    "@graphql-codegen/typed-document-node": "^2.3.11",
    "@graphql-codegen/typescript": "^2.8.6",
    "@graphql-codegen/typescript-operations": "^2.5.11",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@playwright/test": "^1.25.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.44",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "autoprefixer": "^10.4.12",
    "cssnano": "^5.1.13",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^20.0.3",
    "postcss": "^8.4.17",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.53.1",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tailwindcss": "^3.2.2",
    "tslib": "^2.3.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vitest": "^0.25.8"
  },
  "type": "module",
  "dependencies": {
    "@kitql/graphql-codegen": "^0.5.4",
    "@sveltejs/kit": "^1.0.1",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@urql/svelte": "^3.0.1",
    "graphql": "^16.6.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "type-fest": "^3.2.0",
    "uri-template": "^2.0.0",
    "urql": "^3.0.3",
    "yaml": "^2.2.1"
  }
}
